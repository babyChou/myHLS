{
  "compilerOptions": {
    "declaration" : false,
    "target": "es5",
    "module": "es6",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types" : [
      "jquery",
      "underscore"
    ],
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "local_typings"
  ],
  "exclude": [
    "node_modules",
    "mockData"
  ],
  "compileOnSave": true
}

